{
  "apps": {
    "http": {
      "servers": {
        "boreman": {
          "listen": [":443"],
          "routes": [
            {
              "match": [
                {
                  "host": ["anders.webstreams.io"]
                }
              ],
              "handle": [
                {
                  "handler": "reverse_proxy",
                  "upstreams": [
                    {
                      "dial": ":9001"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}
